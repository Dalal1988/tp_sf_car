{% extends "base.html.twig" %}

{% block main %}

<h1>Liste des Brands</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Country</th>
    </tr>
  </thead>
  {% for brand in brands %}
  <tbody>
    <tr>
      <th scope="row">{{ brand.id }}</th>
      <td>{{ brand.name }}</td>
      <td>{{ brand.countrie }}</td>
    </tr>
  </tbody>
  {% endfor %}
</table>
		

{% endblock %}
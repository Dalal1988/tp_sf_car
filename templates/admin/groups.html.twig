{% extends "base.html.twig" %}

{% block main %}


<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Country</th>
    </tr>
  </thead>
  {% for group in groups %}
  <tbody>
    <tr>
      <th scope="row">{{ group.id }}</th>
      <td>{{ group.name }}</td>
      <td>{{ group.countrie }}</td>
    </tr>
  </tbody>
  {% endfor %}
</table>
		

{% endblock %}